{
    "name": "markup/elasticsearch-bundle",
    "description": "A Symfony bundle providing simple integration with the Elasticsearch SDK, also providing web profiler information.",
    "type": "symfony-bundle",
    "keywords": ["elasticsearch", "client", "symfony", "bundle", "search"],
    "require": {
        "php": "^7.1",
        "elasticsearch/elasticsearch": "^6",
        "symfony/framework-bundle": "^3.4|^4",
        "markup/json": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "mockery/mockery": "^1.2",
        "symfony/console": "^3.4|^4",
        "phpstan/phpstan-shim": "^0.10.5"
    },
    "autoload": {
        "psr-4": { "Markup\\ElasticsearchBundle\\": "" }
    },
    "license": "MIT",
    "authors": [
